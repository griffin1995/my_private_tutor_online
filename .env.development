# Phase 2 Multi-Environment Configuration - Development
# CONTEXT7 SOURCE: /vercel/next.js - Environment configuration for optimization

# Environment
NODE_ENV=development
NEXT_PUBLIC_ENV=development

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:3000
NEXT_PUBLIC_API_TIMEOUT=30000

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_MONITORING=true
NEXT_PUBLIC_ENABLE_DEBUG=true
NEXT_PUBLIC_ENABLE_PESTICIDE=false

# Performance Settings
NEXT_PUBLIC_IMAGE_OPTIMIZATION=true
NEXT_PUBLIC_LAZY_LOADING=true
NEXT_PUBLIC_CODE_SPLITTING=true

# Development Tools
NEXT_PUBLIC_SHOW_DEV_TOOLS=true
NEXT_PUBLIC_LOG_LEVEL=debug

# Build Optimization
NEXT_TURBOPACK=true
NEXT_TURBOPACK_TRACING=false

# TypeScript
TSC_COMPILE_ON_ERROR=true
TSC_WATCHFILE=priorityPollingInterval