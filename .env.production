# Phase 2 Multi-Environment Configuration - Production
# CONTEXT7 SOURCE: /vercel/next.js - Environment configuration for optimization

# Environment
NODE_ENV=production
NEXT_PUBLIC_ENV=production

# API Configuration
NEXT_PUBLIC_API_URL=https://myprivatetutoronline.com
NEXT_PUBLIC_API_TIMEOUT=10000

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_MONITORING=true
NEXT_PUBLIC_ENABLE_DEBUG=false
NEXT_PUBLIC_ENABLE_PESTICIDE=false

# Performance Settings
NEXT_PUBLIC_IMAGE_OPTIMIZATION=true
NEXT_PUBLIC_LAZY_LOADING=true
NEXT_PUBLIC_CODE_SPLITTING=true
NEXT_PUBLIC_CDN_ENABLED=true

# Production Settings
NEXT_PUBLIC_SHOW_DEV_TOOLS=false
NEXT_PUBLIC_LOG_LEVEL=error

# Build Optimization
NEXT_TURBOPACK=false
ANALYZE=false

# Security
NEXT_PUBLIC_CSP_ENABLED=true
NEXT_PUBLIC_RATE_LIMITING=true
NEXT_PUBLIC_HSTS_ENABLED=true

# Royal Client Standards
NEXT_PUBLIC_PREMIUM_FEATURES=true
NEXT_PUBLIC_ROYAL_ENDORSEMENTS=true