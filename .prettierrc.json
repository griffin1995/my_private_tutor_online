{
	"bracketSameLine": true,
	"experimentalTernaries": true,
	"jsxSingleQuote": true,
	"proseWrap": "always",
	"singleAttributePerLine": true,
	"singleQuote": true,
	"trailingComma": "all",
	"tabWidth": 1,
	"useTabs": true,
	"plugins": ["prettier-plugin-tailwindcss"],
	"tailwindConfig": "./tailwind.config.ts",
	"tailwindFunctions": ["clsx", "cn", "cva"],
	"semi": false,
	"printWidth": 120,
	"endOfLine": "lf"
}
