{
  "// CONTEXT7 SOURCE": "/vercel/next.js - Incremental compilation cache configuration",
  "// OPTIMIZATION REASON": "Phase 3 build performance optimization for <10s builds",
  "cache": {
    "type": "filesystem",
    "buildDependencies": {
      "config": ["next.config.ts", "tsconfig.json", "package.json"]
    },
    "version": 1,
    "cacheDirectory": ".next/cache",
    "compression": "gzip"
  },
  "incremental": {
    "typescript": true,
    "webpack": true,
    "swc": true
  },
  "parallel": {
    "workers": 4,
    "workerIdleMemoryLimit": 512
  }
}